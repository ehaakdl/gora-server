        },
        {
            "type": "java",
            "name": "ServerApplication",
            "request": "launch",
            "mainClass": "org.gora.server.ServerApplication",
            "projectName": "gora-server",
            "vmArgs": [
                "-Duser.timezone=GMT"
            ],
            "env": {
                "DB_USERNAME": "gora",
                "DB_PASSWORD": 1234,
                "DB_HOST": "localhost",
                "DB_PORT": 3306,
                "DB_NAME": "gora",
                "MAX_DEFAULT_QUE_SZ": 200000,
                "JWT_SECRET_KEY": "a89e2da3-704d-4ff0-a803-c8d8dc57cbf1",
                "TCP_ACCEPT_THREAD_COUNT": 1,
                "TCP_EVENT_THREAD_COUNT": 1,
                "UDP_ACCEPT_EVENT_THREAD_COUNT": 1,
                "CLIENT_DATA_BUFFER_EXPIRE_TIME": 300000,
                "CLIENT_DATA_BUFFER_CLEAN_DELAY": 600000,
                "SERVER_TYPE": "master",
                "ASYNC_CORE_POOL_SIZE": 2,
                "ASYNC_MAX_POOL_SIZE": 2,
                "AES_KEY": "abcewqewqweqwewqeweewqeqwqqwewqd",
                "AES_IV": "qwewqwqewewqwqew"
            }
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "TcpClient",
            "request": "launch",
            "mainClass": "example.netty.TcpClient"
        },
        {
            "type": "java",
            "name": "UdpClient",
            "request": "launch",
            "mainClass": "example.netty.UdpClient"
        }
    ]
}